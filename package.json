  "contributors": [""],
  "resolutions": {
    "@compodoc/compodoc/@angular-devkit/core/chokidar": "^4.0.3"
  },
  "keywords": [
    "angular",
    "angular 19",
    "angular-starter",
    "angular starter",
    "angular-boilerplate",
    "angular boilerplate",
    "angular-template",
    "angular template",
